                  </div>
                  <div class="logged-user-menu color-style-bright">
                    <div class="logged-user-avatar-info">
                      <div class="avatar-w">
                        <img alt="" src="<%= Routes.static_path(@conn, "/images/logos/LAXMI.png") %>">
                      </div>
                      <div class="logged-user-info-w">
                        <div class="logged-user-name">
                          <%= "#{String.capitalize(@conn.assigns.user.first_name)}"%>
                        </div>
                        <div class="logged-user-role">
                          <%= "#{String.capitalize(@conn.assigns.user.user_role)}"%>
                        </div>
                      </div>
                    </div>
                    <div class="bg-icon">
                      <i class="os-icon os-icon-wallet-loaded"></i>
                    </div>
                    <ul>
                      <li>
                        <a href="users_profile_big.html"><i class="os-icon os-icon-user-male-circle2"></i><span>Profile Details</span></a>
                      </li>
                      <li>
                        <a href="<%= Routes.session_path(@conn, :signout)%>"><i class="os-icon os-icon-signs-11"></i><span>Logout</span></a>
                      </li>
                    </ul>
                  </div>
                </div>
              </div>
