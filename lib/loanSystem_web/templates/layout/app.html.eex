            <!--------------------
            END - Top Menu Controls
            -------------------->
          </div>
          <!--------------------
          END - Top Bar
          -------------------->
          <div class="content-i">
            <div class="content-box">
              <main id="js-page-content" role="main" class="page-content">
              <!--------------------------------------------------------------------------------Flash -->
            <div class="row">
                <div class="col-md-12 col-lg-offset-2">
                    <div class="form-group">
                        <%= if get_flash(@conn, :info) do %>
                            <div class="alert alert-styled-left" style="background-color:#AFEEEE;">
                            <button type="button" class="close" data-dismiss="alert">&times; <span class="sr-only">Close</span></button>
                            <%= get_flash(@conn, :info) %>
                            </div>
                        <% end %>
                        <%= if get_flash(@conn, :error) do %>
                            <div class="alert alert-styled-left" style="background-color:#FFE4E1;">
                            <button type="button" class="close" data-dismiss="alert">&times; <span class="sr-only">Close</span></button>
                            Oops! <%= get_flash(@conn, :error) %>
                            </div>
                        <% end %>
                    </div>
                </div>
            </div>
                  <%= render @view_module, @view_template, assigns  %>
              </main>
              <!--------------------
                START - Color Scheme Toggler
                -------------------->
                <div class="floated-colors-btn second-floated-btn">
                  <div class="os-toggler-w">
                    <div class="os-toggler-i">
                      <div class="os-toggler-pill"></div>
                    </div>
